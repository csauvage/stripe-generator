  "bugs": {
    "url": "https://github.com/csauvage/stripe-generator/issues"
  },
  "homepage": "https://github.com/csauvage/stripe-generator#readme",
  "dependencies": {
    "cfonts": "^2.3.0",
    "chalk": "^2.4.1",
    "chance": "^1.0.17",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "configstore": "^4.0.0",
    "figlet": "^1.2.1",
    "inquirer": "^6.2.1",
    "minimist": "^1.2.0",
    "stripe": "^6.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
